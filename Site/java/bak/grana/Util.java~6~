public class Util {

  public Util() {
  }

  public boolean eValido(String s) {

    float f = tofloat(s);
    if (s == null) {
 //     System.out.println("a");
      return false;
    } else if (s.equals("")) {
 //     System.out.println("b");
      return false;
    } else {
 //     System.out.println("c");
      return true;
    }

  }
}

